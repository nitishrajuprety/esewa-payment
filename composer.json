{
    "name": "nitishrajuprety/esewa-payment",
    "description": "Integrate E-Sewa payment gateway in any Laravel project.",
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nitish Raj Uprety",
            "email": "uprety.nitish@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "NitishRajUprety\\EsewaPayment\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12",
        "ext-curl": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NitishRajUprety\\EsewaPayment\\EsewaPaymentServiceProvider"
            ],
            "aliases": {
                "EsewaPayment": "NitishRajUprety\\EsewaPayment\\EsewaPaymentFacade"
            }
        }
    }
}
